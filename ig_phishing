printf $magenta
echo      "
 ░▀█▀░█▀▀░░░░░█▀█░█░█░▀█▀░█▀▀░█░█░▀█▀░█▀█░█▀▀
 ░░█░░█░█░░░░░█▀▀░█▀█░░█░░▀▀█░█▀█░░█░░█░█░█░█
 ░▀▀▀░▀▀▀░▀▀▀░▀░░░▀░▀░▀▀▀░▀▀▀░▀░▀░▀▀▀░▀░▀░▀▀▀"
echo
printf $reset
echo -e   "\e[1;36m  AUTHOR :\033[1;31m   VeNOM24"
echo -e   "\e[1;36m  TOOL   :\033[1;31m   IG_PHISHING"
echo -e   "\e[1;36m  VERSION:\033[1;31m   1.0"

